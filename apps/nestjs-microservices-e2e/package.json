{
  "name": "@myorg/nestjs-microservices-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "implicitDependencies": [
      "@myorg/nestjs-microservices"
    ],
    "targets": {
      "e2e": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "apps/nestjs-microservices-e2e/jest.config.ts",
          "passWithNoTests": true
        },
        "dependsOn": [
          "@myorg/nestjs-microservices:build",
          "@myorg/nestjs-microservices:serve"
        ]
      }
    }
  }
}
